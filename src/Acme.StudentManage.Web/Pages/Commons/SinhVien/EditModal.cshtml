@page
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model Acme.StudentManage.Web.Pages.Commons.SinhVien.EditModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="sinhVien" asp-page="EditModal">
    <abp-modal>
        <abp-modal-header title="Update Lop"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
